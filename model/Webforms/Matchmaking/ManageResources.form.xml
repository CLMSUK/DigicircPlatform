{"Actions":[{"Code":"function void Index(int id) {\r\n\tModel.Actor = Domain.Actor.GetByKey(id);\r\n    Model.Title = \"'\" + Model.Actor.Name + \"' \" + LocalResources.RES_PAGETITLE_Index;\r\n\tModel.IsEdited = false;\r\n\t\r\n\tvar currentUserName = AppLib.Session.GetCurrentUserName();\r\n    \r\n    if(currentUserName != Model.Actor.AddedBy.UserName && Model.Actor.Administrators.Where(a => a.UserName == currentUserName).Length == 0)\r\n    {\r\n    \tShowMessage(\"You don't have permission to edit this actor.\", AppLib.MessageType.Warning, FormModels.ActorForm.Controller.Show.GetLink(id));\r\n    \treturn;\r\n    }\r\n}","Name":"Index","Permissions":["EditOrganisation"],"AllowAnonymous":false,"AllowAllAuthenticatedUsers":false,"AccessLog":false,"IsEntryPoint":true,"IsDefault":false,"CausesValidation":false,"DontPostViewModel":false,"AllGroups":true,"Groups":[]},{"Code":"function void AddNewResource(bool desired)\n{\n\tModel.ModalTitle = \"Add new Resource\";\n\tModel.SelectedProduct = Domain.Product.Create();\n\tModel.SelectedProduct.IsDesired = desired;\n\tModel.IsEdited = false;\n\tFormControls.Modal.Show();\n}","Name":"AddNewResource","Permissions":["EditOrganisation"],"AllowAnonymous":false,"AllowAllAuthenticatedUsers":false,"AccessLog":false,"IsEntryPoint":false,"IsDefault":false,"ResetDirty":false,"CausesValidation":false,"DontPostViewModel":false,"AllGroups":true,"Groups":[]},{"Code":"function void EditResource(Domain.Product product)\n{\n\tModel.ModalTitle = \"Edit \" + product.Resource.Name;\n//\tif(product.IsDesired)\n//\t{\n//\t\tModel.Actor.CircularEconomyRequirements.DesiredResources.Remove(product);\t\n//\t}\n//\telse\n//\t{\n//\t\tModel.Actor.CircularEconomyRequirements.Resources.Remove(product);\n//\t}\n\tModel.IsEdited = true;\n\tModel.SelectedProduct = product;\n\tFormControls.Modal.Show();\n}","Name":"EditResource","Permissions":["EditOrganisation"],"AllowAnonymous":false,"AllowAllAuthenticatedUsers":false,"AccessLog":false,"IsEntryPoint":false